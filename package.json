{
  "name": "macro11-syntax-vscode",
  "publisher": "nzeemin",
  "version": "0.0.2",
  "repository": {
    "url": "https://github.com/nzeemin/macro11-syntax-vscode"
  },
  "displayName": "macro11-syntax-vscode",
  "description": "MACRO11 syntax highlighling for VSCode",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "keywords": [
    "MACRO11",
    "Assembler",
    "PDP-11",
    ".MAC",
    ".INC"
  ],
  "contributes": {
    "languages": [
      {
        "id": "macro11",
        "aliases": [
          "MACRO11",
          "macro11"
        ],
        "extensions": [
          ".MAC",
          ".INC"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "macro11",
        "scopeName": "source.macro11",
        "path": "./syntaxes/macro11.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "uiTheme": "vs-dark",
        "label": "MACRO11 default",
        "path": "./themes/macro11.theme.json"
      }
    ]
  }
}